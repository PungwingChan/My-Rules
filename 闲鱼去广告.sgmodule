[Map Local]

# 开屏广告
^https?:\/\/acs\.m\.goofish\.com\/gw\/mtop\/.+\/splash\.ads\/ data-type=text data="{}" status-code=200 header="Content-Type: application/json"

# 首页推荐流广告（NextFresh）
^https?:\/\/acs\.m\.goofish\.com\/gw\/mtop\/.+\/idlehome\.home\.nextfresh\/ data-type=text data="{}" status-code=200 header="Content-Type: application/json"

# Widget 小组件广告
^https?:\/\/acs\.m\.goofish\.com\/gw\/mtop\/.+\/idlehome\.widget\.refresh\.get\/ data-type=text data="{}" status-code=200 header="Content-Type: application/json"

# 我的页面推广
^https?:\/\/acs\.m\.goofish\.com\/gw\/mtop\/.+\/idle\.user\.page\.my\.adapter\/ data-type=text data="{}" status-code=200 header="Content-Type: application/json"

# 搜索页广告
^https?:\/\/g-?acs\.m\.goofish\.com\/gw\/mtop\/.+\/idlemtopsearch\.search\/ data-type=text data="{}" status-code=200 header="Content-Type: application/json"

