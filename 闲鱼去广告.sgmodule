# 购买页广告
^https?:\/\/acs\.m\.goofish\.com\/gw\/mtop\/.+\/idle\.item\.buy\.feeds\/ data-type=text data="{}" status-code=200 header="Content-Type: application/json"

# 本地生活广告组件
^https?:\/\/acs\.m\.goofish\.com\/gw\/mtop\/.+\/idle\.local\.flow\.plat\.section\/ data-type=text data="{}" status-code=200 header="Content-Type: application/json"

# 首页新卡片广告
^https?:\/\/acs\.m\.goofish\.com\/gw\/mtop\/.+\/idlehome\.home\.newitem\.page\/ data-type=text data="{}" status-code=200 header="Content-Type: application/json"

[MITM]
hostname = %APPEND% acs.m.goofish.com, g-acs.m.goofish.com
